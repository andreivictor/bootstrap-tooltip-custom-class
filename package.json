{
  "name": "bootstrap-tooltip-custom-class",
  "version": "1.1.0",
  "description": "Extends Bootstrap Tooltips and Popovers by adding custom classes.",
  "scripts": {
    "install-v3": "cd bootstrap-v3 && rm -rf node_modules && npm install",
    "install-v4": "cd bootstrap-v4 && rm -rf node_modules && npm install",
    "install-v4-alpha": "cd bootstrap-v4-alpha && rm -rf node_modules && npm install",
    "install-all": "npm run install-v3 && npm run install-v4 && npm run install-v4-alpha",
    "build-v3-tooltip": "grunt buildv3Tooltip",
    "build-v3-popover": "grunt buildv3Popover",
    "build-v3": "npm run build-v3-tooltip && npm run build-v3-popover",
    "build-v4-tooltip": "grunt buildv4Tooltip",
    "build-v4-popover": "grunt buildv4Popover",
    "build-v4": "npm run build-v4-tooltip && npm run build-v4-popover",
    "build-v4-alpha-tooltip": "grunt buildv4AlphaTooltip",
    "build-v4-alpha-popover": "grunt buildv4AlphaPopover",
    "build-v4-alpha": "npm run build-v4-alpha-tooltip && npm run build-v4-alpha-popover",
    "build-all": "npm run build-v3 && npm run build-v4 && npm run build-v4-alpha",
    "version-v3": "cd bootstrap-v3 && json -I -f package.json -e 'this.version=process.env.npm_package_version'",
    "version-v4": "cd bootstrap-v4 && json -I -f package.json -e 'this.version=process.env.npm_package_version'",
    "version-v4-alpha": "cd bootstrap-v4-alpha && json -I -f package.json -e 'this.version=process.env.npm_package_version'",
    "version-all": "npm run version-v3 && npm run version-v4 && npm run version-v4-alpha",
    "serve-v3-tooltip": "grunt serve-v3Tooltip",
    "serve-v3-popover": "grunt serve-v3Popover",
    "serve-v4-tooltip": "grunt serve-v4Tooltip",
    "serve-v4-popover": "grunt serve-v4Popover",
    "serve-v4-alpha-tooltip": "grunt serve-v4AlphaTooltip",
    "serve-v4-alpha-popover": "grunt serve-v4AlphaPopover",
    "serve-all": "grunt serve-all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreivictor/bootstrap-tooltip-custom-class.git"
  },
  "keywords": [
    "bootstrap"
  ],
  "author": {
    "name": "Andrei Victor Bulearca",
    "url": "https://andreivictor.ro/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreivictor/bootstrap-tooltip-custom-class/issues"
  },
  "homepage": "https://github.com/andreivictor/bootstrap-tooltip-custom-class#readme",
  "dependencies": {},
  "devDependencies": {
    "grunt": "^1.0.4",
    "grunt-banner": "^0.6.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-connect": "^2.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-less": "^2.0.0",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^22.0.0",
    "grunt-sass": "^3.1.0",
    "json": "^9.0.6",
    "node-sass": "^4.13.0"
  }
}
